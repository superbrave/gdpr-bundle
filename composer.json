{
  "name"       : "superbrave/gdpr-bundle",
  "type"       : "symfony-bundle",
  "description": "A Symfony bundle for using entity annotations according to GDPR requirements and anonymizing/exporting data",
  "keywords"   : [
    "annotations",
    "GDPR",
    "anonymizing",
    "exporting"
  ],
  "licence"    : "MIT License",
  "require"    : {
    "php": ">=7.1.3",
    "doctrine/annotations": "^1.4",
    "symfony/framework-bundle": "^4.0.0",
    "symfony/options-resolver": "^4.0",
    "symfony/property-access": "^4.0",
    "symfony/serializer": "^4.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "doctrine/collections": "^1.5",
    "phpunit/phpunit": "^7.0",
    "superbrave/coding-standards": "^0.1",
    "squizlabs/php_codesniffer": "^3.2.3"
  },
  "autoload"   : {
    "psr-4": {"Superbrave\\GdprBundle\\": ""}
  },
  "config"      : {
    "bin-dir"    : "bin",
    "platform"   : {
      "php": "7.1.3"
    },
    "sort-packages": true
  }
}
